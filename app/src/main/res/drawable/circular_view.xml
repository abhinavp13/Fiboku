<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="6dp"
        android:right="6dp"
        android:bottom="6dp"
        android:left="6dp"
        >
        <shape android:shape="oval" >
            <solid android:color="@color/colorPrimary" />
        </shape>
    </item>
    <item
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp"
        android:left="5dp">
        <shape
            android:shape="oval">
            <stroke android:width="1dp"
                android:color="#22000000" />
            <size
                android:width="190dp"
                android:height="190dp"/>
        </shape>
    </item>

</layer-list>